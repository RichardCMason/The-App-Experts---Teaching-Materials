   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "np.random.seed(42)\n",
    "x_1 = np.random.normal(loc = 100, scale = 15, size = 100)\n",
    "x_2 = np.random.normal(loc = 110, scale = 5, size = 100)\n",
    "y = x_1 + x_2 + np.random.normal(loc = 0, scale = 25, size = 100)\n",
    "\n",
    "df = pd.DataFrame().from_dict({'y':y,\n",
    "                              'x1':x_1,\n",
    "                              'x2':x_2})"
   ]
  },
